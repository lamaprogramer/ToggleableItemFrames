{
  "schemaVersion": 1,
  "id": "toggleableitemframes",
  "version": "${version}",

  "name": "Toggleable Item Frames",
  "description": "Allows you to easily toggle item frames to be invisible.",
  "authors": [
    "Iamaprogrammer"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/toggle-item-frames",
    "sources": "https://github.com/lamaprogramer/ToggleableItemFrames"
  },

  "license": "MIT License",
  "icon": "assets/toggleableitemframes/icon.png",

  "environment": "*",
  "entrypoints": {
    "modmenu": [
      "net.iamaprogrammer.toggleableitemframes.compat.modmenu.ModMenuCompat"
    ],
    "main": [
      "net.iamaprogrammer.toggleableitemframes.ToggleableItemFrames"
    ],
    "client": [
      "net.iamaprogrammer.toggleableitemframes.ToggleableItemFramesClient"
    ]
  },
  "mixins": [
    "toggleableitemframes.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "minecraft": [
      "1.21.0"
    ],
    "java": ">=21"
  }
}
